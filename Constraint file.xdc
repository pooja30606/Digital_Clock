# Clock
set_property PACKAGE_PIN N11 [get_ports clk1]
set_property IOSTANDARD LVCMOS33 [get_ports clk1]

# Reset
set_property PACKAGE_PIN L5 [get_ports rst]
set_property IOSTANDARD LVCMOS33 [get_ports rst]

# Digit[0]
set_property PACKAGE_PIN F2 [get_ports {out[0]}]
set_property IOSTANDARD LVCMOS33 [get_ports {out[0]}]

# Digit[1]
set_property PACKAGE_PIN E1 [get_ports {out[1]}]
set_property IOSTANDARD LVCMOS33 [get_ports {out[1]}]

# Digit[2]
set_property PACKAGE_PIN G5 [get_ports {out[2]}]
set_property IOSTANDARD LVCMOS33 [get_ports {out[2]}]

# Digit[3]
set_property PACKAGE_PIN G4 [get_ports {out[3]}]
set_property IOSTANDARD LVCMOS33 [get_ports {out[3]}]

# Segment[0]
set_property PACKAGE_PIN H1 [get_ports {seg[0]}]
set_property IOSTANDARD LVCMOS33 [get_ports {seg[0]}]

# Segment[1]
set_property PACKAGE_PIN H2 [get_ports {seg[1]}]
set_property IOSTANDARD LVCMOS33 [get_ports {seg[1]}]

# Segment[2]
set_property PACKAGE_PIN J4 [get_ports {seg[2]}]
set_property IOSTANDARD LVCMOS33 [get_ports {seg[2]}]

# Segment[3]
set_property PACKAGE_PIN J5 [get_ports {seg[3]}]
set_property IOSTANDARD LVCMOS33 [get_ports {seg[3]}]

# Segment[4]
set_property PACKAGE_PIN H4 [get_ports {seg[4]}]
set_property IOSTANDARD LVCMOS33 [get_ports {seg[4]}]

# Segment[5]
set_property PACKAGE_PIN H5 [get_ports {seg[5]}]
set_property IOSTANDARD LVCMOS33 [get_ports {seg[5]}]

# Segment[6]
set_property PACKAGE_PIN G1 [get_ports {seg[6]}]
set_property IOSTANDARD LVCMOS33 [get_ports {seg[6]}]

# Segment[7] (DP)
set_property PACKAGE_PIN G2 [get_ports {seg[7]}]
set_property IOSTANDARD LVCMOS33 [get_ports {seg[7]}]
